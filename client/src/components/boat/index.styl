 .boat {
//  fill-rule: evenodd;
//  clip-rule: evenodd;
  stroke-linejoin: round;
  stroke-miterlimit: 1.414;

  height: 34px;
  width: 34px;
  margin-left: -17px;
  margin-top: -17px;
  transform-origin: 17px 17px 0;

  &__background {
    fill: #6b6b6b;
  }
  &__foreground {
    color: inherit;
    fill: currentColor;
  }

}
